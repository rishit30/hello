{
     "family": "testing-app-task",
     "networkMode": "awsvpc",
     "containerDefinitions": [
       {
         "name": "testing-app-container",
         "image": "${aws_ecr_repository.testing-app_repo.repository_url}:latest",
         "essential": true,
         "portMappings": [
           {
             "containerPort": 8080,
             "hostPort": 8080,
             "protocol": "tcp"
           }
         ]
       }
     ],
     "requiresCompatibilities": ["FARGATE"],
     "cpu": "256",
     "memory": "512"
   }