{
  "family": "my-app",
  "containerDefinitions": [
    {
      "name": "my-app",
      "image": "${AWS_ECR_REPO}:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ]
}